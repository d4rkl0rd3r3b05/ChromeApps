{
	"manifest_version": 2,
	"name": "Magicbricks",
	"description": "Property Sahi Milegi Yahin",
	"version": "1.0",
	"minimum_chrome_version": "40.0.2213.0",
	"icons": {
		"128": "appIcon.png"
	},
	"app": {
	    "background": {
	      "scripts": ["background.js"]
	    }
  	},
	"permissions": [
		"unlimitedStorage",
		"notifications",
		"fullscreen", 
		"alwaysOnTopWindows",
		"webview",
		"geolocation", 
		"usb",
		{
			"usbDevices" : [
						      {
						        "vendorId": 1908,
						        "productId": 1320
						      }
						    ]
		}
	]
}